# docker-compose.yml
version: '3.8'
services:
  postgres:
    image: ankane/pgvector  # pgvector 확장 포함된 PostgreSQL 이미지
    container_name: pgvector-db
    ports:
      - "5432:5432"
    environment:
      POSTGRES_USER: eden
      POSTGRES_PASSWORD: secret123
      POSTGRES_DB: mydb
    volumes:
      - pgdata:/var/lib/postgresql/data
    restart: unless-stopped

volumes:
  pgdata:

